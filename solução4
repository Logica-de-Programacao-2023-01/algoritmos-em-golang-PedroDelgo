package main

import "fmt"

func main() {
	var n1 float64
	var n2 float64
	var n3 float64
	fmt.Print("Qual é o primeiro número: ")
	fmt.Scan(&n1)
	fmt.Print("Qual é o segundo número: ")
	fmt.Scan(&n2)
	fmt.Print("qual é o terceiro número: ")
	fmt.Scan(&n3)
	fmt.Print("A média desses 3 números com os pesos 2, 3 e 5 é: ", (n1*2+n2*3+n3*5)/10)
}
