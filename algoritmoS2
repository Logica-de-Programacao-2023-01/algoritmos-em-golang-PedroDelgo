package main

import "fmt"

func main() {
	var n1 int
	var n2 int
	var n3 int
	fmt.Print("Escreva um número inteiro: ")
	fmt.Scan(&n1)
	fmt.Print("Escreva mais um número inteiro: ")
	fmt.Scan(&n2)
	fmt.Print("Escreva mais um número inteiro: ")
	fmt.Scan(&n3)
	if n1 < n2 && n1 < n3 {
		fmt.Print("O seu priemiro número é o menor ", n1)
	} else if n2 < n1 && n2 < n3 {
		fmt.Print("O seu segundo número é o menor ", n2)
	} else {
		fmt.Print("O seu terceiro número é o menor ", n3)
	}
}

